/*
 * File Name	: main.c
 * Title 		: Basic LED blinking Program
 * Created on	: Mar 18, 2016
 * Author		: root
 * Notes		: Turns on led
 */

#ifndef F_CPU
#define F_CPU 1000000UL
#endif

#include <avr/io.h>
typedef unsigned long u32;

void delay(u32 count);

int main(void)
{
	DDRC|=0xff;
	while(1)
	{
		PORTC|=0xff;
		delay(500);
		PORTC&=~(0xff);
	}
}

//Todo: Write delay function
void delay(u32 count)
{
	while(count--);
}
